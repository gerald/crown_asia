<h1>New Raw Material Type</h1>

<%= render :partial => "shared/error_messages", :locals => {:target => @raw_material_type} %>

<%= form_for @raw_material_type do |f| %>
  <%= render :partial => "form", :locals =>{:f => f} %>
  
  <p class="buttons">
    <%= submit_tag "Save" %>
    <%= button_to_function "Reset", "$('#new_raw_material_type')[0].reset()" %>
    <%= button_to_function "Cancel", "window.location.replace('#{raw_material_types_path}')" %>
  </p>
<% end %>