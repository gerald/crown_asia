<fieldset>
  <dl>
    <dd><%= f.label :supply_type %></dd>
    <dd><%= f.select :supply_type, supply_type_options, {:include_blank => true} %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :name %></dd>
    <dd><%= f.text_field :name %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :description %></dd>
    <dd><%= f.text_area :description %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :active %></dd>
    <dd><%= f.check_box :active %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :compounds %></dd>
    <dd><%= f.check_box :compounds %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :pipes %></dd>
    <dd><%= f.check_box :pipes %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :corporate %></dd>
    <dd><%= f.check_box :corporate %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :unit_of_measure %></dd>
    <dd><%= f.select :unit_of_measure_id, UnitOfMeasure.all(:order => "code").collect{|u| [u.name, u.id]}, {:include_blank => true} %></dd>
  </dl>
  
  <dl>
    <dd><%= f.label :comments %></dd>
    <dd><%= f.text_area :comments %></dd>
  </dl>
</fieldset>