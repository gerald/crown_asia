<tr>
  <% if !@supply_transaction.new_record? %>
    <td><%= f.check_box :_destroy %></td>
  <% end %>
  <td>
    <%= f.select :supply_id, Supply.all(:conditions => ["supply_type = ?", @supply_transaction.supply_type]).collect{|s| [s.name, s.id]}, {:include_blank => true}, {:class => "supply_select"} %>
  </td>
  <td><%= f.text_field :quantity %></td>
</tr>